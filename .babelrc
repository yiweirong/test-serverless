{
  "presets": ["next/babel"],
  "plugins": [
    [
      "import", 
      {
        "libraryName": "antd",
        "libraryDirectory":"lib",
        "style": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "components": "./components",
          "constants": "./constants",
          "apis": "./apis",
          "pages": "./pages",
          "helpers": "./helpers"
        }
      }
    ]
  ]
}